{
	"bindings": [
		{
			"authLevel": "anonymous",
			"type": "httpTrigger",
			"direction": "in",
			"name": "req",
			"methods": ["get"],
			"route": "articles/{id}"
		},
		{
			"type": "http",
			"direction": "out",
			"name": "res"
		},
		{
			"name": "documents",
			"databaseName": "dev",
			"collectionName": "article",
			"connectionStringSetting": "cosmosDbConnetionString",
			"sqlQuery": "SELECT c.id,c.title,c.text FROM c WHERE c.id={id}",
			"direction": "in",
			"type": "cosmosDB"
		}
	],
	"scriptFile": "../dist/getArticles/index.js"
}
